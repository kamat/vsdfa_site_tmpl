<div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
    <div class="frame event">
        <a href="[[~[[+id]]]]">
            <div class="date">
                <span class="day">[[+tv.startDate:strtotime:date=`%e`]]</span>
                <span class="month">[[+tv.startDate:strtotime:date=`%b`:limit=`3`]]</span>
                <span class="year">[[+tv.startDate:strtotime:date=`%Y`]]</span>
            </div>
            <h5 class="description">
                [[+pagetitle]]
            </h5>
        </a>
    </div>
</div>
